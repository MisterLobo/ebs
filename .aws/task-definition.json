{
  "containerDefinitions": [ 
    {
      "command": [
        "./api"
      ],
      "essential": true,
      "image": "645972258043.dkr.ecr.ap-southeast-1.amazonaws.com/ebs-api-test:608cb9b4c9a2f9b7e2273d73c211abeb0652ac11",
      "logConfiguration": { 
        "logDriver": "awslogs",
        "options": { 
          "awslogs-group": "/ecs/TestTDFam",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "name": "test-ecs-app",
      "portMappings": [ 
        {
          "containerPort": 9090,
          "hostPort": 9090,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "cpu": "1024",
  "memory": "3072",
  "executionRoleArn": "arn:aws:iam::012345678910:role/ecsTaskExecutionRole",
  "family": "test-task-definition",
  "networkMode": "awsvpc",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": [ 
    "FARGATE"
  ]
}